{
  "ReverseProxy": {
    "Routes": {
      "moderation-routes": {
        "ClusterId": "moderation-cluster",
        "AuthorizationPolicy": "Admin",
        "Match": {
          "Path": "moderation/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      }
    }
  }
}