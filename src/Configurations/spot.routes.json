{
  "ReverseProxy": {
    "Routes": {
      "add-spot-route": {
        "ClusterId": "spot-cluster",
        "AuthorizationPolicy": "Admin",
        "Match": {
          "Path": "spot/api/spots",
          "Methods": [ "POST" ]
        },
        "Transforms": [ { "PathPattern": "api/spots" } ]
      },
      "submit-spot-route": {
        "ClusterId": "spot-cluster",
        "AuthorizationPolicy": "Authenticated",
        "Match": {
          "Path": "spot/api/spots/submit",
          "Methods": [ "POST" ]
        },
        "Transforms": [ { "PathPattern": "api/spots/submit" } ]
      },
      "delete-spot-route": {
        "ClusterId": "spot-cluster",
        "AuthorizationPolicy": "Owner",
        "Match": {
          "Path": "spot/api/spots",
          "Methods": [ "DELETE" ]
        },
        "Transforms": [ { "PathPattern": "api/spots" } ]
      },
      "mine-submissions-route": {
        "ClusterId": "spot-cluster",
        "AuthorizationPolicy": "Authenticated",
        "Match": {
          "Path": "spot/api/submissions/mine"
        },
        "Transforms": [ { "PathPattern": "api/submissions/mine" } ]
      },
      "get-submission-by-id-route": {
        "ClusterId": "spot-cluster",
        "AuthorizationPolicy": "Authenticated",
        "Match": {
          "Path": "spot/api/submissions/{id}"
        },
        "Transforms": [ { "PathPattern": "api/submissions/{id}" } ]
      },
      "spot-routes": {
        "ClusterId": "spot-cluster",
        "Match": {
          "Path": "spot/{**catch-all}"
        },
        "Transforms": [ { "PathPattern": "{**catch-all}" } ]
      }
    }
  }
}